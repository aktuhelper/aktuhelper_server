{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "course_name": {
      "type": "string"
    },
    "duration": {
      "type": "integer"
    },
    "has_branches": {
      "type": "boolean"
    },
    "branches": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::branch.branch",
      "mappedBy": "course"
    },
    "subjects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subject.subject",
      "mappedBy": "course"
    }
  }
}
